{"paragraphs":[{"text":"/*\nEjercicio 1 (operadores de comparación). Modifica el patrón de la temperatura para que solo se cumpla cuando esta sea mayor de 35 grados y pruébalo con distintos valores\n(necesitaremos añadir la cláusula where, Esper ref. 5.5).\n*/","user":"6c74b07a-8465-4dcb-822a-96259fb2aef1","dateUpdated":"2023-10-20T06:55:21-0400","config":{"colWidth":12,"fontSize":9,"enabled":true,"results":{},"editorSetting":{"language":"epl","editOnDblClick":false,"completionSupport":false},"editorMode":"ace/mode/epl"},"settings":{"params":{},"forms":{}},"apps":[],"jobName":"paragraph_1697799264097_1366715976","id":"20231020-065424_2085181092","dateCreated":"2023-10-20T06:54:24-0400","status":"READY","progressUpdateIntervalMs":500,"focus":true,"$$hashKey":"object:4230"},{"text":"%esperepl\ncreate schema TemperatureSimpleEvent(device String, value Integer);\n\n@Name('AllTemperature')\nselect *\nfrom TemperatureSimpleEvent\nwhere value > 35;","user":"6c74b07a-8465-4dcb-822a-96259fb2aef1","dateUpdated":"2023-10-20T06:28:39-0400","config":{"colWidth":12,"fontSize":9,"enabled":true,"results":{},"editorSetting":{"language":"epl","editOnDblClick":false,"completionSupport":false},"editorMode":"ace/mode/epl"},"settings":{"params":{},"forms":{}},"results":{"code":"SUCCESS","msg":[]},"apps":[],"jobName":"paragraph_1697797699970_200585383","id":"20231020-062819_1927649816","dateCreated":"2023-10-20T06:28:19-0400","dateStarted":"2023-10-20T06:28:39-0400","dateFinished":"2023-10-20T06:28:39-0400","status":"FINISHED","progressUpdateIntervalMs":500,"$$hashKey":"object:4231"},{"text":"%esperscenario\nTemperatureSimpleEvent={device='Fridge', value=5}\nt=t.plus(5 seconds)\nTemperatureSimpleEvent={device='Fridge', value=7}","user":"6c74b07a-8465-4dcb-822a-96259fb2aef1","dateUpdated":"2023-10-20T06:28:39-0400","config":{"colWidth":12,"fontSize":9,"enabled":true,"results":{},"editorSetting":{"language":"epl","editOnDblClick":false,"completionSupport":false},"editorMode":"ace/mode/undefined"},"settings":{"params":{},"forms":{}},"results":{"code":"SUCCESS","msg":[{"type":"HTML","data":"\n<div class=\"container-fluid\">\n<div class=\"row\">\n<div class=\"col-sm-1 small\" style=\"background-color:#f0f0f0; font-weight: bold;\">Time</div>\n<div class=\"col-sm-1 small\" style=\"background-color:#f0f0f0; font-weight: bold;\">Statement</div>\n<div class=\"col-sm-1 small\" style=\"background-color:#f0f0f0; font-weight: bold;\">Stream</div>\n<div class=\"col-sm-9 small\" style=\"background-color:#f0f0f0; font-weight: bold;\">Output Event</div>\n</div>\n</div>\n"}]},"apps":[],"jobName":"paragraph_1697797708212_1880436109","id":"20231020-062828_1715525895","dateCreated":"2023-10-20T06:28:28-0400","dateStarted":"2023-10-20T06:28:39-0400","dateFinished":"2023-10-20T06:28:39-0400","status":"FINISHED","progressUpdateIntervalMs":500,"$$hashKey":"object:4232"}],"name":"ISI/ejercicio01","id":"2JEUDDD81","noteParams":{},"noteForms":{},"angularObjects":{"esper:shared_process":[]},"config":{"isZeppelinNotebookCronEnable":false,"looknfeel":"default","personalizedMode":"false"},"info":{}}